{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AppDataFactory2018"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureDataLakeStorageLogCsv_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageLogCsv'"
		},
		"AzureSqlDatabase_LinkedService1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase_LinkedService1'"
		},
		"AzureDataLakeStorageLogCsv_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datastore2108.dfs.core.windows.net/"
		},
		"AzureSqlDatabase_LinkedService1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "appserver2108.database.windows.net"
		},
		"AzureSqlDatabase_LinkedService1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "appdb"
		},
		"AzureSqlDatabase_LinkedService1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AppDataFactory2018_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureDataLakeStorageLogCsv_accountKey": {
						"value": "[parameters('AzureDataLakeStorageLogCsv_accountKey')]"
					},
					"AzureSqlDatabase_LinkedService1_password": {
						"value": "[parameters('AzureSqlDatabase_LinkedService1_password')]"
					},
					"AzureDataLakeStorageLogCsv_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageLogCsv_properties_typeProperties_url')]"
					},
					"AzureSqlDatabase_LinkedService1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase_LinkedService1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase_LinkedService1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase_LinkedService1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase_LinkedService1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase_LinkedService1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}